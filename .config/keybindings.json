    {
        "key": "ctrl+a k",
        "when": "editorTextFocus && !terminalFocus",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+a left",
        "when": "editorTextFocus && !terminalFocus",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+a h",
        "when": "editorTextFocus && !terminalFocus",
        "command": "workbench.action.navigateLeft"
